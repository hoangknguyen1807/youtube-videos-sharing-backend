        "@types/bull": "3.15.8",
        "@types/cache-manager": "3.4.3",
        "@types/cron": "2.0.0",
        "@types/express": "4.17.13",
        "@types/faker": "6.6.8",
        "@types/jest": "27.5.1",
        "@types/js-yaml": "^4.0.5",
        "@types/lodash": "4.14.182",
        "@types/md5": "^2.3.2",
        "@types/mime-types": "^2.1.1",
        "@types/multer": "1.4.7",
        "@types/node": "^17.0.35",
        "@types/nodemailer": "^6.4.4",
        "@types/passport-jwt": "3.0.6",
        "@types/passport-local": "1.0.34",
