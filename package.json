        "@typescript-eslint/parser": "4.31.1",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "glob": "8.0.3",
        "husky": "^8.0.1",
        "jest": "28.1.0",
        "joi": "17.6.0",
        "lint-staged": "^12.4.1",
        "prettier": "2.6.2",
