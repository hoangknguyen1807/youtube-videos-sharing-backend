{
    "name": "youtube-video-sharing-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": ">=16.x"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "check": "prettier --check \"src/**/*.ts\"",
        "start": "nest start",
        "wz-command": "wz-command",
        "start:dev": "npm run prebuild && nest start --watch",
        "start:debug": "npm run prebuild && nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config jest-e2e.json --testTimeout=60000 --forceExit --detectOpenHandles",
        "test:unit": "jest --config jest-unit.json --testTimeout=60000 --forceExit --detectOpenHandles",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/config/orm.config.ts",
        "typeorm:create-migration": "npm run typeorm -- migration:create -n",
        "typeorm:run-migrations": "npm run typeorm migration:run",
        "typeorm:revert-migration": "npm run typeorm migration:revert"
    },
    "dependencies": {
